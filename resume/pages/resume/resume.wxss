page view button:not([size='mini']),
page button:not([size='mini']){padding:0;margin:0;width:auto;border:none;font-weight: normal;}
.title{font-size: 32rpx;color:#333;font-weight: bold;}
page button.basicInfo{width:calc(100% - 60rpx);display:flex;padding:10rpx 0 30rpx;margin:0 30rpx 30rpx;border:none;border-bottom: 1px solid #eee;justify-content: space-between;}
.basicInfo .left{display: flex;flex-direction: column;}
.basicInfo .left .me{display: flex;margin-bottom: 10rpx;align-items: flex-end;}
.basicInfo .left .me .name{font-size: 48rpx;color:#333;font-weight: 700;margin-right: 10rpx;}
.basicInfo .left .me .name .iconfont{font-size: 24rpx;}
.basicInfo .left .info{display: flex;}
.basicInfo .left .info text{font-size: 28rpx;}
.basicInfo .left .info .year{display: flex;}
.basicInfo .left .info .age{margin:0 10rpx;}
.basicInfo .left .info .age::after,
.basicInfo .left .info .age::before{content: 'Â·';}
.basicInfo .left .info .age::after{margin-left:10rpx;}
.basicInfo .left .info .age::before{margin-right:10rpx;}
.basicInfo .left .info .xueli{display: flex;}
.basicInfo .right{display: flex;}
.basicInfo .right image{width:100rpx;height:100rpx;border-radius: 100%;overflow: hidden;}

